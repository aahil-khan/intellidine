    {
      "name": "üîê Authentication",
      "description": "Customer OTP and Staff Login Endpoints",
      "item": [
        {
          "name": "Customer - Request OTP",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Response has message', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.data).to.have.property('message');",
                  "    pm.expect(jsonData.data.message).to.include('OTP sent');",
                  "    // Save OTP for next request (if available in development)",
                  "    if (jsonData.data.otp) {",
                  "        pm.environment.set('otp_code', jsonData.data.otp);",
                  "    }",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"phone\": \"{{customer_phone}}\",\n  \"tenant_id\": \"{{tenant_id}}\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/customer/request-otp",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "customer", "request-otp"]
            }
          },
          "response": [
            {
              "name": "200 OK",
              "originalRequest": {
                "method": "POST",
                "url": "http://localhost:3001/api/auth/customer/request-otp"
              },
              "status": "OK",
              "code": 200,
              "body": "{\"success\": true, \"message\": \"OTP sent to phone number\", \"expires_in\": 300}"
            }
          ]
        },
        {
          "name": "Customer - Verify OTP",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Status code is 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Response contains JWT token', function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.data).to.have.property('access_token');",
                  "    pm.environment.set('jwt_token', jsonData.data.access_token);",
                  "    pm.environment.set('user_id', jsonData.data.user.id);",
                  "});"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"phone\": \"{{customer_phone}}\",\n  \"otp\": \"{{otp_code}}\",\n  \"tenant_id\": \"{{tenant_id}}\"\n}"
            },
            "url": {
